{
  "name": "io.github.mvolkert.dancingmoves",
  "displayName": "DancingMoves",
  "version": "0.1.17",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:github": "ng build --configuration production --base-href https://mvolkert.github.io",
    "android:build": "ng build --configuration android --base-href ./",
    "android:assembleRelease": "cd android && gradlew assembleRelease",
    "android:bundleRelease": "cd android && gradlew bundleRelease",
    "android:emulate": "npx capacitor run android",
    "android:emulateAll": "npm run android:build && npx cap copy android && npm run android:emulate",
    "android:release": "ng build --configuration android-prod --base-href ./ && npx cap copy android && npm run android:bundleRelease",
    "android:keystore": "keytool -genkey -v -keystore dancingmoves.keystore -alias dancingmovesmobileapps -keyalg RSA -keysize 2048 -validity 10000",
    "android:jarsigner": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore dancingmoves.keystore platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk dancingmovesmobileapps",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "ng deploy --base-href=/dancing-moves/ --no-silent",
    "e2eGenerate": "npx playwright codegen http://localhost:4200/",
    "e2e": "npx playwright test --debug && npx playwright show-report",
    "allure": "npx allure generate allure-results -o allure-report --clean && npx allure open allure-report",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run start",
    "upgrade:angular": "ng update @angular/core @angular/compiler @angular-builders/custom-webpack @angular-devkit/build-angular @angular/cdk @angular/material @angular/cli",
    "upgrade:packges": "npx npm-check-updates --upgrade"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^15.0.3",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/material": "^15.0.3",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "@capacitor/android": "4.7.0",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/app": "4.1.1",
    "@capacitor/core": "4.7.0",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/keyboard": "4.1.1",
    "@capacitor/status-bar": "4.1.1",
    "@codetrix-studio/capacitor-google-auth": "^3.2.2",
    "crypto-browserify": "~3.12.0",
    "crypto-es": "~1.2.7",
    "cytoscape": "~3.23.0",
    "jwt-simple": "~0.5.6",
    "ngx-markdown": "~15.1.1",
    "rxjs": "~7.8.0",
    "stream-browserify": "~3.0.0",
    "tslib": "~2.5.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^15.0.0",
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "^15.0.4",
    "@angular/compiler-cli": "^15.0.4",
    "@bgotink/playwright-coverage": "^0.1.6",
    "@capacitor/cli": "4.7.0",
    "@playwright/test": "^1.31.1",
    "@types/codemirror": "~5.60.7",
    "@types/cytoscape": "~3.19.9",
    "@types/jasmine": "~4.3.1",
    "@types/node": "~16.18.1",
    "@types/uuid": "^9.0.1",
    "allure-commandline": "^2.21.0",
    "allure-playwright": "^2.0.0",
    "angular-cli-ghpages": "~1.0.5",
    "buffer": "~6.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-firefox-launcher": "~2.1.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.4"
  }
}